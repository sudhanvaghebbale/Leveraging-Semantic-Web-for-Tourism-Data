<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".LandingPage">

    <Button
            android:text="Submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/gpsSubmitButton"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="342dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="161dp" android:layout_marginStart="161dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/gpsXEditText"
            android:hint="Enter GPS latitude" android:layout_marginBottom="49dp"
            app:layout_constraintBottom_toTopOf="@+id/gpsYEditText" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="99dp" android:layout_marginStart="99dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/gpsYEditText"
            android:hint="Enter GPS longitude" android:layout_marginBottom="83dp"
            app:layout_constraintBottom_toTopOf="@+id/gpsSubmitButton" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="99dp" android:layout_marginStart="99dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>